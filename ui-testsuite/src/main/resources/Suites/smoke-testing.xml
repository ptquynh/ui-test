<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Smoke Testing" verbose="1" preserve-order="true">
	<test name="smoke-testing" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
			<class name="com.tekexperts.selenium.smoke.TestConfig_LoginOut" />
			<class name="com.tekexperts.selenium.smoke.Smoke_LoginOut" />
			<class name="com.tekexperts.selenium.smoke.TestConfig_Smoke" />
			<class name="com.tekexperts.selenium.smoke.Smoke_BusinessManagement" />
			<class name="com.tekexperts.selenium.smoke.Smoke_Organization" />
			<class name="com.tekexperts.selenium.smoke.Smoke_PipelineManagement" />
			<class name="com.tekexperts.selenium.smoke.Smoke_DataImport" />
			<class name="com.tekexperts.selenium.smoke.Smoke_Dashboards" />
			<class name="com.tekexperts.selenium.smoke.Smoke_HistoricalData" />
		</classes>
	</test>
	<listeners>
		<listener class-name="com.tekexperts.pipeline.common.TestReportListener" />
	</listeners>
</suite>