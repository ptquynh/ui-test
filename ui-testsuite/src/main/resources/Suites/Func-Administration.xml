<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Smoke Testing" verbose="1" preserve-order="true">
	<test name="smoke-testing" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
		    <class name="com.tekexperts.selenium.refreshData.TestConfig" />
		    <class name="com.tekexperts.selenium.refreshData.Refresh_Assignment_Rules" />
			<class name="com.tekexperts.selenium.func.administration.TestConfig" />
			<class name="com.tekexperts.selenium.func.administration.Func_Assignment_Rule" />
			<class name="com.tekexperts.selenium.func.administration.Func_Organization_User" />
			<class name="com.tekexperts.selenium.func.administration.Func_Business_Unit" />
		</classes>
	</test>
	<listeners>
		<listener class-name="com.tekexperts.pipeline.common.TestReportListener" />
	</listeners>
</suite>