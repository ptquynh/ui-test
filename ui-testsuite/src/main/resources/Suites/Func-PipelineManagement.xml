<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Smoke Testing" verbose="1" preserve-order="true">
	<test name="smoke-testing" enabled="true">
		<groups>
			<run>
				<exclude name="pending" />
			</run>
		</groups>
		<classes>
		    <class name="com.tekexperts.selenium.func.pipelineManagement.TestConfig" />
		    <class name="com.tekexperts.selenium.func.pipelineManagement.Func_Pipeline_List" />
			<class name="com.tekexperts.selenium.func.pipelineManagement.Func_Pipeline_Setting" />
			<class name="com.tekexperts.selenium.func.pipelineManagement.Func_Unassign_Contracts" />
		</classes>
	</test>
	<listeners>
		<listener class-name="com.tekexperts.pipeline.common.TestReportListener" />
	</listeners>
</suite>